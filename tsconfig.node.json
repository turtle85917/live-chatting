{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2017",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "ES2017",
      "ESNext.AsyncIterable"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}